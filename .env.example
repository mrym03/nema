# API Keys for Zero Waste Pantry App
# Copy this file to .env and add your actual keys

# OpenAI API key for AI features
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Spoonacular API for recipe information
EXPO_PUBLIC_SPOONACULAR_API_KEY=your_spoonacular_api_key_here

# TheMealDB API (free tier uses "1")
EXPO_PUBLIC_THEMEALDB_API_KEY=1

# Google Cloud Vision API (if using)
EXPO_PUBLIC_GOOGLE_CLOUD_VISION_API_KEY=your_vision_api_key_here

# UPC Database API for barcode scanning
EXPO_PUBLIC_UPC_DATABASE_API_KEY=your_upc_database_api_key_here

# Feature flags
EXPO_PUBLIC_USE_MOCK_OCR=false 